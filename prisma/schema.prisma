generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id          Int      @id @default(autoincrement())
  user_id     Int
  text        String
  create_time DateTime @default(now())
  users       users?   @relation(fields: [user_id], references: [id])
}

model users {
  id    Int     @id @default(autoincrement())
  name  String
  posts posts[]
}
